{
  "swagger": "2.0",
  "info": {
    "version": "1.0.2",
    "title": "RMV Homes Teamwork API",
    "description": "an api reference documentation for a robust integration between Teamwork Projects APIs (Versions 1, 2 and 3) and other software solutions RMV Homes uses.",
    "contact": {
      "name": "RMV Homes Admin"
    }
  },
  "host": "rmvprojects.teamwork.com",
  "basePath": "/login",
  "schemes": [
    "http"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/user": {
      "get": {
        "description": "Sample endpoint: Returns details about a particular user",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "userId",
            "type": "integer",
            "required": true,
            "description": "ID of the user"
          }
        ],
        "responses": {
          "200": {
            "description": "Sample response: Details about a user by ID",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/User"
              }
            }
          }
        }
      }
    }
  },
  "definitions": {
    "User": {
      "type": "object",
      "required": [
        "name"
      ],
      "properties": {
        "id": {
          "type": "integer",
          "format": "int64"
        },
        "name": {
          "type": "string"
        },
        "tag": {
          "type": "string"
        }
      }
    }
  }
}







